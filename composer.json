{
    "name": "azamatx/wp-profiling",
    "description": "Tool for profiling and debugging WordPress with clockwork",
    "authors": [
        {
            "name": "Azamat X",
            "email": "azamat@stylemix.net"
        }
    ],
    "require": {
        "itsgoingd/clockwork": "^4.0@stable",
        "yetanotherape/diff-match-patch": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Stylemix\\WPClockwork\\": "src/"
        }
    },
    "config": {
        "secure-http": false,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}
